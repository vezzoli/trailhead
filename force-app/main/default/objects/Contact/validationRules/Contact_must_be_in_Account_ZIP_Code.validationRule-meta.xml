<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contact_must_be_in_Account_ZIP_Code</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT( ISBLANK( AccountId ) ),MailingPostalCode  &lt;&gt;  Account.ShippingPostalCode)</errorConditionFormula>
    <errorMessage>the contact is related to an account and has a mailing postal code different</errorMessage>
</ValidationRule>
